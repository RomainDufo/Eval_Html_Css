<script>
    const switchButtons = document.querySelector(".banner-switch-buttons");
    let index = 1;
    const bannerUrls = [
        "https://www.adrar-formation.com/wp-content/uploads/2022/10/propal_slider_HQ.jpeg",
        "https://www.adrar-formation.com/wp-content/uploads/2022/10/propal_slider2_HQ.jpeg",
        "https://www.adrar-formation.com/wp-content/uploads/2022/09/propal_slider3_HQ.png"
    ];
    const interval = setInterval(() => {
        if (window.innerWidth > 980) {
            document.querySelectorAll('.active-banner').forEach((element) => {
                element.classList.remove("active-banner");
            });
            switchButtons.children[index].classList.add("active-banner");
            document.querySelector(".changing-hero").style.backgroundImage = `url("${bannerUrls[index]}")`;
            index++;
            if (index === 3) index = 0;
        }
    }, 5000);

    function switchBanner(e, bannerUrl) {
        clearInterval(interval);
        document.querySelectorAll('.active-banner').forEach((element) => {
            element.classList.remove("active-banner");
        });
        e.classList.add("active-banner");
        document.querySelector(".changing-hero").style.backgroundImage = `url("${bannerUrl}")`;
    }

</script>

